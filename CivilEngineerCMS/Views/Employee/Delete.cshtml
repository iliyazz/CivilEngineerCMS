

@model EmployeePreDeleteViewModel
@{
    ViewBag.Title = "Delete project";
}
<h2 class="text-center">@ViewBag.Title</h2>
<hr/>
<form method="post" asp-action="Delete">
    <div class="row">
        <div class="col-md-8">
            <div class="form-group">
                <label asp-for="FirstName" class="control-label"></label>
                <input asp-for="FirstName" class="form-control" disabled="disabled"/>
            </div>
            <div class="form-group">
                <label asp-for="LastName" class="control-label"></label>
                <input asp-for="LastName" class="form-control" disabled="disabled" />
            </div>
            <div class="form-group">
                <label asp-for="JobTitle" class="control-label"></label>
                <input asp-for="JobTitle" class="form-control" disabled="disabled"/>
            </div>
            <div class="form-group">
                <label asp-for="Email" class="control-label"></label>
                <input asp-for="Email" class="form-control" disabled="disabled"/>
            </div>
            <div class="form-group">
                <label asp-for="Address" class="control-label"></label>
                <input asp-for="Address" class="form-control" disabled="disabled"/>
            </div>
            <div class="form-group mt-2">
                <input type="submit" value="Delete" class="btn btn-danger mt-3"/>
                <a class="btn btn-primary mt-3" href="javascript: history.back();">Cancel</a>
            </div>
        </div>
    </div>
</form>